{
  "name": "@cml/google-maps-scraper",
  "version": "1.0.0",
  "description": "A fast and reliable Google Maps scraping tool for Node.js. Extract business listings including names, ratings, reviews, phone numbers, and Google Maps URLs. Includes a CLI and Excel export.",
  "main": "src/index.js",
  "module": "src/index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.js"
  },
  "bin": {
    "gmaps-scraper": "./bin/gmaps-scraper.js"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules node node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "rm -rf dist coverage *.xlsx",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "google",
    "google-maps",
    "google-maps-scraper",
    "google-maps-api",
    "gmaps",
    "maps",
    "maps-scraper",
    "data-scraper",
    "web-scraper",
    "scraper",
    "scraping",
    "puppeteer",
    "puppeteer-scraper",
    "lead-generation",
    "business-data",
    "business-leads",
    "local-business",
    "local-seo",
    "seo-tools",
    "excel",
    "xlsx",
    "excel-export",
    "cli",
    "automation",
    "data-extraction",
    "crawler",
    "nodejs",
    "node-cli",
    "open-source"
  ],
  "author": "Jerry Satpathy <rutuparna.satpathy01@gmail.com>",
  "maintainers": [
    {
      "name": "Code Media Labs",
      "email": "hello@codemedialabs.in"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Code-Media-Labs/google-maps-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/Code-Media-Labs/google-maps-scraper/issues"
  },
  "homepage": "https://github.com/Code-Media-Labs/google-maps-scraper#readme",
  "funding": {
    "type": "sponsor",
    "url": "https://codemedialabs.in"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "puppeteer": "^24.30.0",
    "exceljs": "^4.4.0",
    "commander": "^14.0.2",
    "chalk": "^5.6.2",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "eslint": "^9.39.1",
    "prettier": "^3.3.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1"
  }
}
